<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">
    <context:annotation-config />
    <context:component-scan base-package="com.tmall.wireless.tac.biz.processor" />

    <bean id="testService" class="com.tmall.recommend.framework.TestService"/>



    <context:component-scan base-package="com.alibaba.cola" />
    <bean class="com.tmall.wireless.tac.biz.processor.AppColaBootstrap" init-method="init">
        <property name="packages" >
            <list>
                <value>com.tmall.recommend.framework</value>
                <value>com.tmall.wireless.tac.biz.processor.ext</value>
            </list>
        </property>
    </bean>
</beans>